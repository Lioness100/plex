{
  "name": "@ctrl/plex",
  "version": "0.0.0-placeholder",
  "description": "plex api client in typescript",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "scttcper/plex",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "plex",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc",
    "test": "jest --runInBand",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "bootstraptest": "ts-node scripts/bootstraptest.ts",
    "teardowntest": "ts-node scripts/teardowntest.ts",
    "claim-server": "ts-node scripts/claim-server.ts",
    "add-media": "ts-node scripts/add-media.ts",
    "test-cleanup": "ts-node scripts/test-cleanup.ts"
  },
  "dependencies": {
    "@ctrl/mac-address": "^2.1.2",
    "got": "^11.8.3",
    "p-any": "^3.0.0",
    "type-fest": "^2.6.0",
    "ws": "^8.3.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@ctrl/eslint-config": "3.1.3",
    "@jest/globals": "27.4.0",
    "@types/lodash": "4.14.177",
    "@types/micromatch": "4.0.2",
    "@types/node": "16.11.11",
    "@types/ws": "8.2.1",
    "@types/xml2js": "0.4.9",
    "@types/yargs": "17.0.7",
    "delay": "5.0.0",
    "execa": "5.1.1",
    "glob": "7.2.0",
    "globby": "11.0.4",
    "jest": "27.4.0",
    "jest-junit": "13.0.0",
    "lodash": "4.17.21",
    "make-dir": "3.1.0",
    "ora": "5.4.1",
    "p-retry": "4.6.1",
    "ts-node": "10.4.0",
    "typedoc": "0.22.10",
    "typescript": "4.5.2",
    "yargs": "17.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageProvider": "v8",
    "testTimeout": 15000
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10"
  }
}
